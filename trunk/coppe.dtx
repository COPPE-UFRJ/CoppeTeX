\newcommand\department[1]{%
  \ifthenelse{\equal{#1}{PEB}}
    {\global\def\@deptpt{Engenharia Biom{\' e}dica}
     \global\def\@depten{Biomedical Engineering}}{}
  \ifthenelse{\equal{#1}{PEC}}
    {\global\def\@deptpt{Engenharia Civil}
     \global\def\@depten{Civil Engineering}}{}
  \ifthenelse{\equal{#1}{PEE}}
    {\global\def\@deptpt{Engenharia El{\' e}trica}
     \global\def\@depten{Electrical Engineering}}{}
  \ifthenelse{\equal{#1}{PEM}}
    {\global\def\@deptpt{Engenharia Mec{\^ a}nica}
     \global\def\@depten{Mechanical Engineering}}{}
  \ifthenelse{\equal{#1}{PEMM}}
    {\global\def\@deptpt{Engenharia Metal{\' u}rgica e de Materiais}
     \global\def\@depten{Metallurgical and Materials Science Engineering}}{}
  \ifthenelse{\equal{#1}{PEN}}
    {\global\def\@deptpt{Engenharia Nuclear}
     \global\def\@depten{Nuclear Engineering}}{}
  \ifthenelse{\equal{#1}{PENO}}
    {\global\def\@deptpt{Engenharia Oce{\^ a}nica}
     \global\def\@depten{Ocean Engineering}}{}
  \ifthenelse{\equal{#1}{PPE}}
    {\global\def\@deptpt{Planejamento Energ{\' e}tico}
     \global\def\@depten{Energy Planning}}{}
  \ifthenelse{\equal{#1}{PEP}}
    {\global\def\@deptpt{Engenharia de Produ{\c c}{\~ a}o}
     \global\def\@depten{Production Engineering}}{}
  \ifthenelse{\equal{#1}{PEQ}}
    {\global\def\@deptpt{Engenharia Qu{\' i}mica}
     \global\def\@depten{Chemical Engineering}}{}
  \ifthenelse{\equal{#1}{PESC}}
    {\global\def\@deptpt{Engenharia de Sistemas e Computa{\c c}{\~ a}o}
     \global\def\@depten{Systems Engineering and Computer Science}}{}
  \ifthenelse{\equal{#1}{PET}}
    {\global\def\@deptpt{Engenharia de Transportes}
     \global\def\@depten{Transportation Engineering}}{}
}
