  \global\let\and\relax%
}
\newcommand\makecatalog{%
  \thispagestyle{plain}
  \setcounter{page}{2}
  \begin{center}
    \begin{minipage}[c]{42ex}
      \setlength{\parindent}{0.55cm}
      \vskip 8.0cm
     \noindent \sloppy\nohyphens{\MakeUppercase\@authsurn},
      \nohyphens{\MakeUppercase\@authname}\par
      \@titulo\ [Rio de Janeiro] \number\year\par
      \pageref{front:pageno},
      \pageref{main:pageno}
      p. $29,7$cm (COPPE/UFRJ, \@degree,
      \nohyphens{\@deptpt}, \number\year)\par
      \@doctypept\ --
      \nohyphens{Universidade Federal do Rio de Janeiro, COPPE}\par
      \baselineskip 0pt
      \begin{enumerate}
        \setlength{\itemindent}{-2.7ex}
        \count1=0
        \@whilenum \count1<\value{keywords} \do {%
          \item \csname K:\the\count1 \endcsname \par
          \advance\count1 by 1}
        \end{enumerate}\par
      I. COPPE/UFRJ\qquad II. T{\' i}tulo (s{\' e}rie)
    \end{minipage}
  \end{center}
}
