          \@coadtitle\ \@coadvisor, \@coaddegree
        \fi
        \count1=0
        \@whilenum \count1<\@examinat \do {%
            \vskip16\p@ \baselineskip14pt
            \makebox[\textwidth]{\hrulefill}
