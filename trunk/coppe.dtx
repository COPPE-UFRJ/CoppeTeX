% It was fixed a maximum of 6 examinators
\newcount\@examinat\@examinat0
\newcommand\examinator[3]{%
  \global\@namedef{S:\expandafter\the\@examinat}{#1\ #2, #3}
  \global\advance\@examinat by 1
  \ifnum\@examinat>6
    \message{coppe.cls: warning: maximum number of examinators exceeded.}
    \message{coppe.cls: warning: see documentation for details.}
    \@examinat=6
  \fi
}
