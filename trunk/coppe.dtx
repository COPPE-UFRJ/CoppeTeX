  \end{flushright}
  \vfill
  \begin{center}
    RIO DE JANEIRO -- RJ, BRASIL\par
    {\MakeUppercase\@monthpt\ DE \number\year}%
  \end{center}
  \end{titlepage}%
  \ifthenelse{\boolean{maledoc}}{}{\makecatalog}%
